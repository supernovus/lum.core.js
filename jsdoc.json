{
  "tags": 
  {
    "allowUnknownTags": false
  },
  "source":
  {
    "include": ["./lib","./docs/src"]
  },
  "opts": 
  {
    "destination": "./docs/api",
    "template": "node_modules/docdash",
    "package": "package.json",
    "readme": "docs/build/README.md",
    "recurse": true
  },
  "plugins": 
  [
    "plugins/markdown"
  ],
  "templates": 
  {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": 
    {
      "outputSourceFiles": true,
      "useLongnameInNav": true
    },
    "navType": "vertical",
    "linenums": true,
    "dateFormat": "YYYY-MM-DD, hh:mm:ss"
  },
  "docdash":
  {
    "__from": "https://github.com/clenemt/docdash",
    "collapse": true,
    "commonNav": true,
    "private": false,
    "search": false,
    "sectionOrder":
    [
      "Modules",
      "Namespaces",
      "Classes",
      "Interfaces",
      "Mixins",
      "Events",
      "Externals",
      "Tutorials"
    ],
    "shortenTypes": false,
    "scopeInOutputPath": false,
    "nameInOutputPath": false,
    "versionInOutputPath": false
  }
}
